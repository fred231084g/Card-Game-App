<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background2"
    tools:context=".Avatar">

    <TextView
        android:id="@+id/textView"
        android:layout_width="289dp"
        android:layout_height="112dp"
        android:gravity="center"
        android:text="@string/avatar"
        android:textColor="@color/white"
        android:textSize="48sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.058" />

    <androidx.gridlayout.widget.GridLayout
        android:id="@+id/avatar_Grid"
        android:layout_width="408dp"
        android:layout_height="495dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageButton
            android:id="@+id/female_6"
            android:layout_width="87dp"
            android:layout_height="134dp"
            app:layout_column="3"
            app:layout_row="2"
            app:srcCompat="@drawable/female_29"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/male_30"
            android:layout_width="112dp"
            android:layout_height="137dp"
            app:layout_column="0"
            app:layout_row="2"
            app:srcCompat="@drawable/male_30"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/male_123"
            android:layout_width="101dp"
            android:layout_height="wrap_content"
            app:layout_column="2"
            app:layout_row="1"
            app:srcCompat="@drawable/male_123"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/male_122"
            android:layout_width="105dp"
            android:layout_height="wrap_content"
            app:layout_column="1"
            app:layout_row="1"
            app:srcCompat="@drawable/male_122"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/female_1"
            android:layout_width="104dp"
            android:layout_height="wrap_content"
            app:layout_column="0"
            app:layout_row="0"
            app:srcCompat="@drawable/female_1"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/female_3"
            android:layout_width="105dp"
            android:layout_height="wrap_content"
            app:layout_column="1"
            app:layout_row="0"
            app:srcCompat="@drawable/female_3"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/female_2"
            android:layout_width="96dp"
            android:layout_height="wrap_content"
            app:layout_column="2"
            app:layout_row="0"
            app:srcCompat="@drawable/female_2"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/female_7"
            android:layout_width="85dp"
            android:layout_height="wrap_content"
            app:layout_column="3"
            app:layout_row="0"
            app:srcCompat="@drawable/female_7"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/male_121"
            android:layout_width="109dp"
            android:layout_height="wrap_content"
            app:layout_column="0"
            app:layout_row="1"
            app:srcCompat="@drawable/male_121"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/male_124"
            android:layout_width="85dp"
            android:layout_height="wrap_content"
            app:layout_column="3"
            app:layout_row="1"
            app:srcCompat="@drawable/male_124"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/male_31"
            android:layout_width="110dp"
            android:layout_height="134dp"
            app:layout_column="1"
            app:layout_row="2"
            app:srcCompat="@drawable/male_31"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/female_25"
            android:layout_width="97dp"
            android:layout_height="132dp"
            app:layout_column="2"
            app:layout_row="2"
            app:srcCompat="@drawable/female_25"
            tools:ignore="SpeakableTextPresentCheck" />
    </androidx.gridlayout.widget.GridLayout>

</androidx.constraintlayout.widget.ConstraintLayout>